{
  "name": "compiler-test",
  "version": "0.1.0",
  "scripts": {
    "build": "bsb -make-world",
    "start": "bsb -make-world -w -ws _ ",
    "clean": "bsb -clean-world",
    "server": "moduleserve ./dev --port 8000",
    "dev": "concurrently \"webpack-dev-server\" \"webpack -w\" ",
    "test": "npm run build && jest",
    "sass": "sass ./sass/index.scss ./build/css/index.css --watch  --load-path=node_modules/",
    "sass:production": "sass ./sass/index.scss ./build/css/index.css --style=compressed",
    "webpack:production": "NODE_ENV=production webpack",
    "deploy": "npm run build; npm run sass:production; npm run webpack:production"
  },
  "keywords": [
    "BuckleScript"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@glennsl/bs-jest": "^0.5.0",
    "react": "^16.8.1",
    "react-dom": "^16.8.1",
    "reason-react": ">=0.7.0",
    "reductive": "^2.0.1",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11"
  },
  "devDependencies": {
    "bs-platform": "^7.1.0",
    "moduleserve": "^0.9.0",
    "webpack-dev-server": "^3.10.3"
  }
}
